#include<iostream>
#include<bits/stdc++.h>
class student{
public:
  string name;
  int id;
  student(){
}
student(string passcode,string name,int id){
this->name=name;
this->id=id;
this->passcode=passcode;
}
 void into(){
cout<<"my name"<<name<<", my id"<<id<<"passcode is"<<passcode<<endl;
}
void setpass(string s)
{
passcode=s;
}
};
int main()
{
student s1;
student s2("10","mohit",1);
student s3;
s3=s2;
//s3+s2
s3.into();
